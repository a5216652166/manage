extends index

block content
  //- button.btn.btn-success(type='button') 添加
  //- br
  //- br
  div.ip-div
    div.panel.panel-warning(style='width:250px')
      div.panel-heading
        | 远程控制卡IP 
      table.table.table-hover.table-condensed
        thead
          tr
            th IP地址
            th 状态
            th 服务编码
        tbody#ip_0
          -for(i = 1; i <= 255; i++){
              tr(data-ip='192.168.0.#{i}')
                td 192.168.0.#{i}
                td
                td
          -}
